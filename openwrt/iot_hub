config attribute 'bootVersion'
    option json_key 'bootVersion'
    option data_type 'string'
    option unit 'none'
    option string_max_len '64'
    option readwrite '0'
    option description 'Firmware U-boot Version'
    # Modbus: 字符串通常放在 Holding，长度根据 max_len/2 计算
    option modbus_enable '1'
    option modbus_table 'holding'
    option modbus_offset '1000'
    option modbus_register_count '32' # 64 bytes / 2
    option modbus_mapping_mode 'string'
    option modbus_scale '1'
    # BACnet: CharacterString Value (CV)
    option bacnet_enable '1'
    option bacnet_object_type 'CV'
    option bacnet_instance_offset '1'
    option bacnet_unit '95'

