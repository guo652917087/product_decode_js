    option default_keep_alive_interval '86400'
    option default_modbus_tcp_enable '0'
    option default_modbus_tcp_port '502'
    option default_modbus_tcp_slave_id '2'
    option default_bacnet_bip_enable '0'
    option default_bacnet_bip_id '101'
